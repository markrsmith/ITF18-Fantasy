#this is the tuple for the monsters making it random
monsterValues = random.randint (0,8)
monsterTuple = ("Goblin", "Theif", "Evil Wizard","Baby Dragon", "Evil Cube Kevin", "Thanos Car", "Kevin's brother the dodecahedron", "Enchanted Danger Noodle", "Velvetra")
#Hp depends on class
if ClassType == 1:
  PlayerHealth = 20
elif ClassType == 2:
  PlayerHealth == 50
elif ClassType == 3:
  PlayerHealth == 30
elif ClassType == 4:
  PlayerHealth == 25
#This is deciding Monster Health
if monsterTuple[monsterValues] == 0:
  monsterHealth = 20
elif monsterTuple[monsterValues] == 1:
  monsterHealth = 15
elif monsterTuple[monsterValues] == 2:
  monsterHealth = 10
elif monsterTuple[monsterValues] == 3:
  monsterHealth = 25
elif monsterTuple[monsterValues] == 4:
  monsterHealth =  15
elif monsterTuple[monsterValues] == 5:
  monsterHealth = 30
elif monsterTuple[monsterValues] == 6:
  monsterHealth =  16
elif monsterTuple[monsterValues] == 7:
  monsterHealth = 24
elif monsterTuple[monsterValues] == 8:
  monsterHealth = 27
#This is the fight sequence portion
while True:
  print("A wild ", monsterTuple[monsterValues]," appeared!")
  print("Do you want to hit the",monsterTuple[monsterValues],"? press n to exit")
  ChoIce = input(str("Yes or no? "))
  ChoIce = (ChoIce[0])
  if ChoIce.lower() == "y":
    if monsterTuple[monsterValues] == 1 :
      Goblin()
